{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    // key: 0 = allow, 1 = warn, 2 = error
    "comma-dangle": [0, "always-multiline"],
    "global-strict": [0],
    "no-process-exit": [0],
    "no-underscore-dangle": [0],
    "no-use-before-define": [0],
    "quotes": [1, "single", "avoid-escape"],
    "strict": [0]
  }
}
